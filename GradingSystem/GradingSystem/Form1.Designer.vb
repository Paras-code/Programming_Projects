    Private Sub btnCgrade_Click(sender As Object, e As EventArgs) Handles btnCgrade.Click
        Dim ww, pt, qe, grade As Single

        ww = (Val(Me.txtW1.Text) + Val(Me.txtW2.Text) + Val(Me.txtW3.Text)) / 3
        pt = (Val(Me.txtPT1.Text) + Val(Me.txtPT2.Text) + Val(Me.txtPT3.Text)) / 3
        qe = Val(Me.txtQexam.Text)
        grade = ww * 0.25 + pt * 0.5 + qe * 0.25

        Me.Height = 586
        Me.lblSubject.Text = Me.txtSubject.Text
        Me.lblSection.Text = "Grade" + Me.cboGrade.Text + "" + "Section" + Me.cboSection.Text
        Me.lblSname.Text = Me.txtSname.Text
        Me.lblGrade.Text = grade




